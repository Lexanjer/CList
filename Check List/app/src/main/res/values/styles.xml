<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar" />

    <style name="AppDefault" parent="AppTheme">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorControlNormal">@color/colorWhite</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>



    <style name="AppLight" parent="AppTheme">
    <item name="colorPrimary">@color/colorPrimaryAppLight</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDarkAppLight</item>
    <item name="colorControlNormal">@color/colorBlack</item>
    </style>

    <style name="AppDark" parent="AppTheme">
        <item name="colorPrimary">@color/colorPrimaryAppDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkAppDark</item>
				<item name="colorControlNormal">@color/colorWhite</item>
				
    </style>



</resources>
